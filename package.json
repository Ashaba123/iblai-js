{
  "name": "@iblai/iblai-sdk",
  "version": "1.0.0",
  "description": "TypeScript SDK for IBL AI Platform - Data layer, utilities, and React components",
  "private": true,
  "keywords": [
    "iblai",
    "sdk",
    "typescript",
    "react",
    "data-layer",
    "web-utils",
    "components"
  ],
  "author": "IBL AI <dev@ibl.ai>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/iblai/iblai-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/iblai/iblai-sdk/issues"
  },
  "homepage": "https://github.com/iblai/iblai-sdk#readme",
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=9.0.0"
  },
  "packageManager": "pnpm@10.11.0",
  "scripts": {
    "build": "turbo run build",
    "build:packages": "turbo run build --filter './packages/*'",
    "dev": "turbo run dev",
    "watch": "turbo run watch",
    "watch:data-layer": "pnpm --filter @iblai/data-layer watch",
    "watch:web-utils": "pnpm --filter @iblai/web-utils watch",
    "watch:web-containers": "pnpm --filter @iblai/web-containers watch",
    "lint": "turbo run lint",
    "lint:fix": "turbo run lint -- --fix",
    "typecheck": "turbo run typecheck",
    "test": "turbo run test",
    "test:watch": "turbo run test:watch",
    "test:coverage": "turbo run test:coverage",
    "format": "prettier --write \"**/*.{ts,tsx,md,json}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,md,json}\"",
    "clean": "turbo run clean && rm -rf node_modules .turbo",
    "changeset": "changeset",
    "changeset:version": "changeset version",
    "changeset:publish": "pnpm build:packages && changeset publish",
    "prepare": "husky"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.7",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@vitest/coverage-v8": "^3.2.4",
    "husky": "9.1.7",
    "lint-staged": "^16.2.6",
    "prettier": "3.5.3",
    "turbo": "2.5.3",
    "typescript": "^5.9.3",
    "vitest": "^3.2.4"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.{md,json}": [
      "prettier --write"
    ]
  }
}
