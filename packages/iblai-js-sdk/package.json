{
  "name": "@iblai/iblai-js-sdk",
  "version": "1.0.0",
  "description": "TypeScript SDK for IBL AI Platform - Unified package for data layer, utilities, and React components",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "module": "dist/index.esm.js",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "rollup -c",
    "watch": "rollup -c -w",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "iblai",
    "sdk",
    "typescript",
    "react",
    "data-layer",
    "web-utils",
    "components",
    "rtk-query",
    "education",
    "ai",
    "mentor"
  ],
  "author": "IBL AI <dev@ibl.ai>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/iblai/iblai-sdk.git",
    "directory": "packages/iblai-js-sdk"
  },
  "bugs": {
    "url": "https://github.com/iblai/iblai-sdk/issues"
  },
  "homepage": "https://github.com/iblai/iblai-sdk#readme",
  "dependencies": {
    "@iblai/data-layer": "^0.3.0",
    "@iblai/web-utils": "^0.3.0",
    "@iblai/web-containers": "^0.3.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "25.0.8",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "15.3.1",
    "@rollup/plugin-typescript": "11.1.6",
    "@types/node": "^22.19.0",
    "@types/react": "19.1.17",
    "rollup": "4.40.0",
    "rollup-plugin-dts": "^6.2.3",
    "rollup-plugin-peer-deps-external": "2.2.4",
    "rollup-plugin-typescript2": "0.36.0",
    "tslib": "2.8.1",
    "typescript": "5.8.3"
  },
  "peerDependencies": {
    "@reduxjs/toolkit": "^2.0.0",
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0",
    "react-redux": "^9.0.0"
  },
  "packageManager": "pnpm@10.11.0",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
